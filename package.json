{
  "name": "cis-api",
  "version": "0.0.2",
  "description": "nestjs-api-websocket",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "repository": "https://git.trinitypro.ru/trinity/api",
  "scripts": {
    "parse": "node -r dotenv/config parser/",
    "format": "prettier --write \"**/*.ts\"",
    "start": "NODE_ENV=prod ts-node -r tsconfig-paths/register src/main.ts",
    "start-dev": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "nodemon dist/src/main.js",
    "start:hmr": "node dist/server",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js",
    "coveralls": "yarn run test:cov --coverageReporters=text-lcov | coveralls",
    "lint": "eslint src/**/*.ts src/**/**/*.ts --fix"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.5",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.3.2",
    "@nestjs/jwt": "^7.1.0",
    "@nestjs/microservices": "^7.3.2",
    "@nestjs/passport": "^7.1.0",
    "@nestjs/platform-express": "^7.6.5",
    "@nestjs/platform-socket.io": "^7.6.13",
    "@nestjs/platform-ws": "^7.6.6",
    "@nestjs/schedule": "^0.4.0",
    "@nestjs/testing": "^7.3.2",
    "@nestjs/typeorm": "^7.1.0",
    "@nestjs/websockets": "^7.6.6",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/express": "^4.17.7",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.27",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "add": "^2.0.6",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "coveralls": "^3.1.0",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-prettier": "^3.3.1",
    "fastify-formbody": "^4.0.3",
    "jest": "^26.1.0",
    "minimist": "1.2.5",
    "moment": "^2.29.1",
    "mysql": "^2.18.1",
    "nest-winston": "^1.4.0",
    "nestjs-config": "^1.4.7",
    "node-code-generator": "^1.0.3",
    "nodemon": "^2.0.4",
    "passport": "^0.4.1",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.3.0",
    "prettier": "^2.2.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.6.0",
    "slugify": "^1.4.5",
    "supertest": "^4.0.2",
    "ts-jest": "^26.1.4",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typeorm": "^0.2.25",
    "typescript": "^3.9.7",
    "typescript-eslint": "0.0.1-alpha.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.0",
    "winston": "^3.3.3",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "eslint": "^7.18.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-import": "^2.22.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "collectCoverageFrom": [
      "!src/config/**",
      "!src/models/**",
      "!src/entities/**"
    ]
  }
}
